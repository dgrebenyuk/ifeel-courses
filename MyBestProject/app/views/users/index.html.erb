<h1>Users#index</h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Age</th>
    <th>Created At</th>
    <th>Updated At</th>
    <th>Links</th>
  </tr>

  <% @users.each do |user| %>
    <%= render partial: 'table_row', locals: { user: user } %>
    <%= render 'table_row', user: user %>
  <% end %>
</table>

<%= render @users %>
